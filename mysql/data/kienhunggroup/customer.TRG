triggers='CREATE DEFINER=`root`@`localhost` TRIGGER `update_created_modified_dates6` BEFORE INSERT ON `customer` FOR EACH ROW BEGIN\n    SET NEW.CreatedDate = NOW();\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER before_insert_customer\nBEFORE INSERT ON customer\nFOR EACH ROW\nBEGIN\n    IF NEW.Customer_Address IS NULL AND NEW.Customer_Phone IS NULL AND NEW.Customer_description IS NULL AND NEW.CreatedBy IS NULL THEN\n        SET NEW.IsFromContact = 3;\n    ELSEIF NEW.Customer_Name IS NOT NULL AND NEW.Customer_Phone IS NOT NULL AND NEW.Customer_description IS NOT NULL AND NEW.Customer_Address IS NULL AND NEW.Customer_Email IS NULL AND NEW.CreatedBy IS NULL THEN\n        SET NEW.IsFromContact = 1;\n    END IF;\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER before_insert_customer_IsFromContact\nBEFORE INSERT ON customer\nFOR EACH ROW\nBEGIN\n    IF NEW.Customer_Address IS NULL AND NEW.Customer_Phone IS NULL AND NEW.Customer_description IS NULL AND NEW.CreatedBy IS NULL THEN\n        SET NEW.IsFromContact = 3;\n    ELSEIF NEW.Customer_Name IS NOT NULL AND NEW.Customer_Phone IS NOT NULL AND NEW.Customer_description IS NOT NULL AND NEW.Customer_Address IS NULL AND NEW.Customer_Email IS NULL AND NEW.CreatedBy IS NULL THEN\n        SET NEW.IsFromContact = 1;\n    END IF;\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `update_modified_date_on_editcu` BEFORE UPDATE ON `customer` FOR EACH ROW\nBEGIN\n    SET NEW.ModifiedDate = NOW();\nEND'
